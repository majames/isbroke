  "devDependencies": {
    "@types/jest": "^20.0.7",
    "jest": "^20.0.4",
    "node-supervisor": "^1.0.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "ts-jest": "^20.0.10"
  },
  "dependencies": {
    "@types/node": "^8.0.24",
    "typescript": "^2.4.2"
  }
}
