{
  "name": "isbroke",
  "version": "0.0.1",
  "description": "Auto detects breaking change in public API of a TypeScript library",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "build:watch": "tsc -w -p .",
    "run": "nodemon ./lib/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "run-p build:watch run"
  },
